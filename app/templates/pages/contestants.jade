.contestants-page
  .container.page
    h1.loc Concurenti
    .teams
      each team in teams.asmi
        .row
          each member in team
            .col-sm-6: .content
              .team.member.image
                each image, index in member.images
                  if (index === 0)
                    a(href="#{image}", data-lightbox='contestants-asmi-#{member.name}', data-title='#{member.name}')
                      img.img-responsive(src="#{image}")
                  else
                    a(href="#{image}", data-lightbox='contestants-asmi-#{member.name}', data-title='#{member.name}')
              .team.member.name
                h3 #{member.name}
                div
                  iframe(src='//www.facebook.com/plugins/like.php?href='+ encodeURIComponent('http://bal.asaa.as-mi.ro/contestant/#{member.number}/') + '&width&layout=button_count&action=like&show_faces=false&share=false&height=35&appId=696798200416054', scrolling='no', frameborder='0', style='border: none; overflow: hidden; height: 35px;', allowtransparency='true')
              .team.member.description
                p !{member.description}
      .teams
        each team in teams.asaa
          .row
            each member in team
              .col-sm-6: .content
                .team.member.image
                  each image, index in member.images
                    if (index === 0)
                      a(href="#{image}", data-lightbox='contestants-asmi-#{member.name}', data-title='#{member.name}')
                        img.img-responsive(src="#{image}")
                    else
                      a(href="#{image}", data-lightbox='contestants-asmi-#{member.name}', data-title='#{member.name}')
                .team.member.name
                  h3 #{member.name}
                  div
                    iframe(src='//www.facebook.com/plugins/like.php?href='+ encodeURIComponent('http://bal.asaa.as-mi.ro/contestant/#{member.number}/') + '&width&layout=button&action=like&show_faces=false&share=false&height=35&appId=696798200416054', scrolling='no', frameborder='0', style='border: none; overflow: hidden; height: 35px;', allowtransparency='true')
                .team.member.description
                  p !{member.description}

